<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:cardElevation="10dp"
    app:cardCornerRadius="2dp"
    app:cardPreventCornerOverlap="false"
    android:layout_marginBottom="5dp"
    android:layout_marginTop="5dp"
    >

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <de.hdodenhof.circleimageview.CircleImageView
            android:id="@+id/bilde"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_marginStart="16dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="1.0"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="5dp">

        </de.hdodenhof.circleimageview.CircleImageView>

        <TextView
            android:id="@+id/navn"
            android:layout_width="193dp"
            android:layout_height="22dp"
            android:text=""
            android:textAlignment="textStart"
            android:textColor="#000"
            android:textSize="19sp"

            app:layout_constraintBottom_toTopOf="@id/lok"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.106"
            app:layout_constraintStart_toEndOf="@+id/bilde"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/lok"
            android:layout_width="193dp"
            android:layout_height="22dp"
            android:text=""
            android:textAlignment="textStart"
            android:textSize="15sp"

            app:layout_constraintBottom_toTopOf="@id/alder"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.106"
            app:layout_constraintStart_toEndOf="@+id/bilde"
            app:layout_constraintTop_toBottomOf="@+id/navn" />

        <TextView
            android:id="@+id/alder"
            android:layout_width="193dp"
            android:layout_height="22dp"
            android:text=""
            android:textAlignment="textStart"
            android:textSize="15sp"

            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.106"
            app:layout_constraintStart_toEndOf="@+id/bilde"
            app:layout_constraintTop_toBottomOf="@+id/lok" />
    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.cardview.widget.CardView>